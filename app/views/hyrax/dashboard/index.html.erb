<%= provide :page_header do %>
    <%= render "hyrax/dashboard/_index_partials/heading_greetings" %>
    <%= render "hyrax/dashboard/_index_partials/heading_actions" %>
<% end %>

<%= content_for :sidebar do %>
  <%= render 'hyrax/users/user', user: current_user %>
  <%= render(partial: "dashboard/_index_partials/export_metadata") if current_user.admin? %>
<% end %>

<%= render "hyrax/dashboard/_index_partials/contents" %>
